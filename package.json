{
  "name": "vocabulary-master",
  "version": "1.0.0",
  "description": "An intelligent vocabulary learning application with spaced repetition",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui"
    "format": "prettier --write \"src/**/*.{vue,js,ts,json,css}\""
  },
  "dependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.0",
    "vuex": "^4.0.0",
    "pinia": "^2.1.0",
    "vue-i18n": "^9.0.0",
    "@vueuse/core": "^10.0.0",
    "@vueuse/head": "^2.0.0",
    "date-fns": "^3.0.0",
    "idb": "^8.0.0",
    "localforage": "^1.10.0",
    "axios": "^1.6.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^23.0.0",
    "prettier": "^3.0.0",
    "typescript": "~5.2.0",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^1.8.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/vocabulary-master.git"
  },
  "keywords": [
    "vocabulary",
    "learning",
    "spaced-repetition",
    "vue",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-username/vocabulary-master/issues"
  },
  "homepage": "https://github.com/your-username/vocabulary-master#readme"
  }
